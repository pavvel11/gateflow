{
  "name": "admin-panel",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:smoke": "playwright test tests/smoke --workers=2",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:api": "vitest run --config vitest.config.api.ts",
    "test:api:watch": "vitest --config vitest.config.api.ts",
    "test:config": "vitest run --config vitest.config.config.ts",
    "t": "vitest run && playwright test tests/smoke --workers=2",
    "tt": "vitest run",
    "ttt": "playwright test --project=chromium && RATE_LIMIT_TEST_MODE=true playwright test --project=rate-limiting --project=rate-limiting-v1",
    "tttt": "cd .. && npx supabase db reset && cd admin-panel && playwright test --project=chromium && RATE_LIMIT_TEST_MODE=true playwright test --project=rate-limiting --project=rate-limiting-v1"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "@marsidev/react-turnstile": "^1.3.1",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.45.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "bir1": "^4.1.0",
    "date-fns": "^4.1.0",
    "geist": "^1.5.1",
    "javascript-obfuscator": "^5.1.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "next-intl": "^4.5.6",
    "react": "^19.2.0",
    "react-confetti": "^6.4.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.6.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "stripe": "^20.0.0",
    "swagger-ui-react": "^5.31.0",
    "terser": "^5.44.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autocannon": "^8.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.0.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
